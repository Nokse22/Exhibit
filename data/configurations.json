{
    "general": {
		"name": "General",
        "formats": ".*()",
        "view-settings": {},
        "other-settings": {}
    },
    "3d_printing": {
        "name": "3D Printing",
        "formats": ".*(3ds|stl|off|3mf)",
        "view-settings": {
            "up": "+Z",
            "grid": true,
            "grid-absolute": true,
            "translucency-support": true,
            "comp": -1,
            "cells": true
        },
        "other-settings": {}
    },
    "mechanical": {
        "name": "Mechanical",
        "formats": ".*(step|stp|iges|igs)",
        "view-settings": {
            "up": "+Z",
            "ambient-occlusion": true,
            "comp": -1,
            "cells": true
        },
        "other-settings": {}
    },
    "splat": {
        "name": "Splat",
        "formats": ".*(splat)",
        "view-settings": {
            "point-sprites": true,
            "point-size": 1,
            "comp": -2,
            "cells": false,
            "bg-color": "0, 0, 0",
            "up": "-Y",
            "camera-position": "0,1,-5.2",
            "camera-focal-point": "0,1,0",
            "tone-mapping": false,
            "grid": false,
            "anti-aliasing": false,
            "translucency-support": false
        },
        "other-settings": {}
    },
    "point_cloud": {
        "name": "Point Cloud",
        "formats": ".*(ply)",
        "view-settings": {
            "up": "-X",
            "comp": -2,
            "translucency-support": true,
            "cells": false,
            "grid": false
        },
        "other-settings": {
            "point-up": false
        }
    }
}