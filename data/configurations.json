{
    "general": {
	"name": "General",
        "formats": ".*()",
        "view-settings": {},
        "other-settings": {}
    },
    "3d_printing": {
        "name": "3D Printing",
        "formats": ".*(3ds|stl|off|3mf)",
        "view-settings": {
            "up": "+Z",
            "grid": true,
            "grid-absolute": true,
            "translucency-support": true
        },
        "other-settings": {}
    },
    "mechanical": {
        "name": "CAD Models",
        "formats": ".*(step|stp|iges|igs)",
        "view-settings": {
            "up": "+Z",
            "ambient-occlusion": false
        },
        "other-settings": {}
    },
    "splat": {
        "name": "Splat",
        "formats": ".*(splat)",
        "view-settings": {
            "point-sprites": true,
            "point-size": 1.0,
            "comp": -2,
            "cells": false,
            "up": "-Y",
            "tone-mapping": false,
            "grid": false,
            "anti-aliasing": false,
            "translucency-support": false
        },
        "other-settings": {}
    },
    "point_cloud": {
        "name": "Point Cloud",
        "formats": ".*(ply)",
        "view-settings": {
			"point-sprites": true,
			"point-size": 0.0,
            "up": "-X",
            "comp": -2,
            "translucency-support": true,
            "cells": false,
            "grid": false
        },
        "other-settings": {
            "point-up": false
        }
    }
}