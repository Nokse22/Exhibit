<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="exhibit">
	<schema id="io.github.nokse22.Exhibit" path="/io/github/nokse22/Viewer3D/">
	  <key name="use-color" type="b">
      <default>false</default>
    </key>
	  <key name="auto-up-dir" type="b">
      <default>true</default>
    </key>
	  <key name="theme" type="s">
      <choices>
        <choice value="follow"/>
        <choice value="light"/>
        <choice value="dark"/>
      </choices>
      <default>'follow'</default>
    </key>
	  <key name="startup-width" type="i">
      <default>840</default>
    </key>
	  <key name="startup-height" type="i">
      <default>600</default>
    </key>
	  <key name="startup-sidebar-show" type="b">
      <default>true</default>
    </key>
	</schema>
</schemalist>
